{
  "expo": {
    "name": "adhd-hub",
    "slug": "adhd-hub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.mycompany.adhdhubi",
      "supportsTablet": true
    },
    "android": {
      "bundleIdentifier": "com.mycompany.adhdhuba",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mycompany.adhdhubi"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "googleServicesFile": "./ios/GoogleService-Info.plist"
          },
          "android": {
            "googleServicesFile": "./android/google-services.json"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ca062ee3-007d-4734-afaa-5073719b5601"
      }
    },
    "owner": "muhanned_21"
  }
}
